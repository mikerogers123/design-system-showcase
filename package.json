{
  "name": "design-system-showcase",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:ci": "ng test --coverage --ci --no-cache",
    "lint": "ng lint","e2e": "ng e2e",
    "e2e:ci": "ng e2e -c ci"
  },
  "license": "None",
  "private": true,
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "htmlWhitespaceSensitivity": "ignore",
    "printWidth": 120
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-reports",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "@angular/cdk": "^10.0.2",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@ukho/components": "^1.5.3",
    "@ukho/styles": "^1.5.2",
    "@angular/animations": "~10.0.4",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/forms": "~10.0.4",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "@ngneat/reactive-forms": "^1.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {"@angular-devkit/build-angular": "~0.1000.3",
    "@angular-eslint/builder": "0.0.1-alpha.32",
    "@angular/cli": "~10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@briebug/cypress-schematic": "^3.3.0",
    "@cypress/webpack-preprocessor": "5.4.1",
    "@types/cucumber": "^4.0.7",
    "@types/jest": "^26.0.4",
    "@types/node": "^12.11.1",
    "@ukho/eslint-config": "^1.0.5",
    "cucumber": "^5.1.0",
    "cypress": "4.10.0",
    "cypress-cucumber-preprocessor": "^1.17.0",
    "eslint": "^7.4.0",
    "jest": "^26.1.0",
    "jest-expect-message": "^1.0.2",
    "prettier": "^2.0.5",
    "ts-loader": "8.0.1",
    "ts-node": "~8.3.0",
    "typescript": "~3.9.5"
  }
}
